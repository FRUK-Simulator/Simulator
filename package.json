    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "react-syntax-highlighter": "^15.5.0",
    "redux": "^5.0.1",
    "styled-components": "^6.1.8",
